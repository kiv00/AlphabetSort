all: bin/main.exe
test: bin/test.exe

bin/test.exe: build/test/main.o build/src/func.o build/test/test_func.o
	gcc -Wall -Werror -o bin/test.exe build/test/main.o build/src/func.o build/test/test_func.o

bin/main.exe: build/src/main.o build/src/func.o
	gcc -Wall -Werror -o bin/main.exe build/src/main.o build/src/func.o 


build/test/main.o: test/main.c
	gcc -c -o build/test/main.o test/main.c

build/test/test_func.o: test/test_func.c
	gcc -c -o build/test/test_func.o test/test_func.c


build/src/main.o: src/main.c
	gcc -c -o build/src/main.o src/main.c 

build/src/func.o: src/func.c
	gcc -c -o build/src/func.o src/func.c 



clean:
	rm -rf bin/*exe build/src/*o build/test/*o